{
	"name": "f3",
	"version": "0.0.4",
	"private": false,
	"description": "Facilitating conversations using Fist to Five.",
	"author": "yoshixmk",
	"main": "index.js",
	"scripts": {
		"build": "yarn clean && concurrently 'npm:build:*'",
		"build:client": "vite build",
		"build:server": "yarn compile",
		"clean": "rimraf dist",
		"compile": "tsc",
		"dev": "concurrently 'npm:dev:*'",
		"dev:client": "vite",
		"dev:server": "ts-node-dev --respawn src/server/index.ts",
		"lint": "rome check",
		"lint-fix": "rome check --apply",
		"start": "concurrently 'npm:start:*'",
		"start:client": "vite serve --mode production",
		"start:server": "node dist/server/index.js"
	},
	"dependencies": {
		"apexcharts": "^3.20.0",
		"axios": "^0.20.0",
		"bootstrap": "^5.0.0-alpha1",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"popper.js": "^1.16.1",
		"vue": "3.0.0-rc.10",
		"vuex": "^4.0.0-beta.4"
	},
	"devDependencies": {
		"@types/cors": "^2.8.7",
		"@types/node": "14.6.4",
		"@vue/compiler-sfc": "3.0.0-rc.10",
		"concurrently": "^5.3.0",
		"rimraf": "^3.0.2",
		"rome": "^10.0.4-beta",
		"ts-node": "^9.0.0",
		"ts-node-dev": "1.0.0-pre.62",
		"typescript": "^4.0.2",
		"vite": "^1.0.0-rc.4"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yoshixmk/f3"
	}
}